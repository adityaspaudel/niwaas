# ==============================
# Node / Express.js .gitignore
# ==============================

# Node modules
node_modules/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# dotenv environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# OS files
.DS_Store
Thumbs.db

# Coverage / testing
coverage/
.nyc_output/

# Build outputs
dist/
build/
tmp/
temp/

# IDE / Editor files
.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# TypeScript (if using)
*.tsbuildinfo
tsconfig.tsbuildinfo

# Optional cache directories
.cache/
.parcel-cache/
.eslintcache

# Dependency directories
bower_components/

# Optional: if you use uploads
uploads/
uploads/*

# Misc
*.swp
*.swo
*.bak
*.tmp

# ==============================
# End of file
# ==============================